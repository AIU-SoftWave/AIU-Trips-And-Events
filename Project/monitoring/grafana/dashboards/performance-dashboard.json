{
  "title": "AIU Trips & Events - Performance Dashboard",
  "tags": [
    "performance",
    "spring-boot",
    "low-latency"
  ],
  "timezone": "browser",
  "editable": true,
  "hideControls": false,
  "graphTooltip": 1,
  "panels": [
    {
      "id": 1,
      "title": "Request Rate (RPS)",
      "type": "graph",
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "expr": "sum(rate(http_server_requests_seconds_count[1m]))",
          "legendFormat": "Total RPS",
          "refId": "A"
        },
        {
          "expr": "sum(rate(http_server_requests_seconds_count{status=~\"2..\"}[1m]))",
          "legendFormat": "Success (2xx)",
          "refId": "B"
        },
        {
          "expr": "sum(rate(http_server_requests_seconds_count{status=~\"5..\"}[1m]))",
          "legendFormat": "Errors (5xx)",
          "refId": "C"
        }
      ],
      "yaxes": [
        {
          "format": "reqps",
          "label": "Requests/sec"
        },
        {
          "format": "short"
        }
      ]
    },
    {
      "id": 2,
      "title": "Response Time Percentiles",
      "type": "graph",
      "gridPos": {
        "x": 12,
        "y": 0,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(http_server_requests_seconds_bucket[1m])) by (le))",
          "legendFormat": "P50 (Median)",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_server_requests_seconds_bucket[1m])) by (le))",
          "legendFormat": "P95 (Target < 200ms)",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(http_server_requests_seconds_bucket[1m])) by (le))",
          "legendFormat": "P99",
          "refId": "C"
        }
      ],
      "yaxes": [
        {
          "format": "s",
          "label": "Response Time"
        },
        {
          "format": "short"
        }
      ],
      "alert": {
        "conditions": [
          {
            "evaluator": {
              "params": [
                0.2
              ],
              "type": "gt"
            },
            "operator": {
              "type": "and"
            },
            "query": {
              "params": [
                "B",
                "5m",
                "now"
              ]
            },
            "reducer": {
              "params": [],
              "type": "avg"
            },
            "type": "query"
          }
        ],
        "name": "P95 Latency Alert"
      }
    },
    {
      "id": 3,
      "title": "Response Time by Endpoint",
      "type": "graph",
      "gridPos": {
        "x": 0,
        "y": 8,
        "w": 24,
        "h": 8
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_server_requests_seconds_bucket[1m])) by (le, uri))",
          "legendFormat": "{{uri}} - P95",
          "refId": "A"
        }
      ],
      "yaxes": [
        {
          "format": "s",
          "label": "Response Time (P95)"
        },
        {
          "format": "short"
        }
      ]
    },
    {
      "id": 4,
      "title": "JVM Memory Usage",
      "type": "graph",
      "gridPos": {
        "x": 0,
        "y": 16,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "expr": "jvm_memory_used_bytes{area=\"heap\"}",
          "legendFormat": "Heap Used",
          "refId": "A"
        },
        {
          "expr": "jvm_memory_max_bytes{area=\"heap\"}",
          "legendFormat": "Heap Max",
          "refId": "B"
        },
        {
          "expr": "jvm_memory_used_bytes{area=\"nonheap\"}",
          "legendFormat": "Non-Heap Used",
          "refId": "C"
        }
      ],
      "yaxes": [
        {
          "format": "bytes",
          "label": "Memory"
        },
        {
          "format": "short"
        }
      ]
    },
    {
      "id": 5,
      "title": "Garbage Collection",
      "type": "graph",
      "gridPos": {
        "x": 12,
        "y": 16,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "expr": "rate(jvm_gc_pause_seconds_count[1m])",
          "legendFormat": "{{gc}} - Collections/sec",
          "refId": "A"
        },
        {
          "expr": "rate(jvm_gc_pause_seconds_sum[1m])",
          "legendFormat": "{{gc}} - Time/sec",
          "refId": "B"
        }
      ],
      "yaxes": [
        {
          "format": "short",
          "label": "Collections/sec"
        },
        {
          "format": "s",
          "label": "Time"
        }
      ]
    },
    {
      "id": 6,
      "title": "Database Connection Pool",
      "type": "graph",
      "gridPos": {
        "x": 0,
        "y": 24,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "expr": "hikaricp_connections_active",
          "legendFormat": "Active Connections",
          "refId": "A"
        },
        {
          "expr": "hikaricp_connections_idle",
          "legendFormat": "Idle Connections",
          "refId": "B"
        },
        {
          "expr": "hikaricp_connections_max",
          "legendFormat": "Max Connections",
          "refId": "C"
        }
      ],
      "yaxes": [
        {
          "format": "short",
          "label": "Connections"
        },
        {
          "format": "short"
        }
      ]
    },
    {
      "id": 7,
      "title": "Database Connection Acquisition Time",
      "type": "graph",
      "gridPos": {
        "x": 12,
        "y": 24,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "expr": "sum(rate(hikaricp_connections_acquire_seconds_sum[1m])) / sum(rate(hikaricp_connections_acquire_seconds_count[1m]))",
          "legendFormat": "Avg Acquire Time (1m)",
          "refId": "A"
        }
      ],
      "lines": true,
      "linewidth": 2,
      "fill": 1,
      "points": false,
      "nullPointMode": "connected",
      "yaxes": [
        {
          "format": "s",
          "label": "Time"
        },
        {
          "format": "short"
        }
      ]
    },
    {
      "id": 8,
      "title": "CPU Usage",
      "type": "graph",
      "gridPos": {
        "x": 0,
        "y": 32,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "expr": "process_cpu_usage * 100",
          "legendFormat": "Application CPU %",
          "refId": "A"
        },
        {
          "expr": "100 - (avg by (instance)(rate(node_cpu_seconds_total{mode=\"idle\"}[1m])) * 100)",
          "legendFormat": "System CPU % (node)",
          "refId": "B"
        }
      ],
      "lines": true,
      "linewidth": 2,
      "fill": 1,
      "points": false,
      "nullPointMode": "connected",
      "yaxes": [
        {
          "format": "percent",
          "label": "CPU Usage",
          "max": 100
        },
        {
          "format": "short"
        }
      ]
    },
    {
      "id": 9,
      "title": "Thread Count",
      "type": "graph",
      "gridPos": {
        "x": 12,
        "y": 32,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "expr": "jvm_threads_live_threads",
          "legendFormat": "Live Threads",
          "refId": "A"
        },
        {
          "expr": "jvm_threads_daemon_threads",
          "legendFormat": "Daemon Threads",
          "refId": "B"
        }
      ],
      "lines": true,
      "linewidth": 2,
      "fill": 1,
      "points": false,
      "nullPointMode": "connected",
      "yaxes": [
        {
          "format": "short",
          "label": "Threads"
        },
        {
          "format": "short"
        }
      ]
    },
    {
      "id": 10,
      "title": "Error Rate",
      "type": "graph",
      "gridPos": {
        "x": 0,
        "y": 40,
        "w": 24,
        "h": 8
      },
      "targets": [
        {
          "expr": "(sum(rate(http_server_requests_seconds_count{status=~\"5..\"}[1m])) / sum(rate(http_server_requests_seconds_count[1m]))) * 100",
          "legendFormat": "Error Rate %",
          "refId": "A"
        }
      ],
      "lines": true,
      "linewidth": 2,
      "fill": 1,
      "points": false,
      "nullPointMode": "connected",
      "yaxes": [
        {
          "format": "percent",
          "label": "Error Rate"
        },
        {
          "format": "short"
        }
      ]
    }
  ],
  "refresh": "5s",
  "schemaVersion": 27,
  "version": 1,
  "links": []
}